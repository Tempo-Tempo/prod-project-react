{
   "typescript.tsdk": "node_modules\\typescript\\lib",
   "i18n-ally.localesPaths": [
      "public/locales",
      "src/shared/i18n"
   ],
   "[scss]": {
      "editor.defaultFormatter": "stylelint.vscode-stylelint"
      },
      "css.validate": false,
      "less.validate": false,
      "scss.validate": false,
      "stylelint.enable": true,
      "stylelint.validate": [
      "css",
      "scss"
      ],
      "editor.codeActionsOnSave": {
         "source.fixAll.stylelint": "explicit"
      },
}